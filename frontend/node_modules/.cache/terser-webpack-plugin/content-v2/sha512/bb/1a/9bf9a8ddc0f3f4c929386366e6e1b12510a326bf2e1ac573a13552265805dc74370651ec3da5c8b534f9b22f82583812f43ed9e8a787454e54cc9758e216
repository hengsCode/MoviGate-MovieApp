 <img\r\n                  className=\"movie-grid-poster\"\r\n                  src={movie.poster_path}\r\n                  alt=\"\"\r\n                />\r\n                <div className=\"movie-grid-overview-container\">\r\n                  <div className=\"movie-grid-overview\">{movie.overview}</div>\r\n                </div>\r\n              </div>\r\n              <div className=\"movie-grid-title\">\r\n                {movie.title}{\" \"}\r\n                <div style={{ color: scaleColour(10 * movie.vote_average) }}>\r\n                  <img\r\n                    className=\"movie-grid-star\"\r\n                    src=\"https://image.flaticon.com/icons/png/512/1828/1828884.png\"\r\n                    alt=\"\"\r\n                  />{\" \"}\r\n                  {movie.vote_average}\r\n                </div>\r\n              </div>\r\n            </div>\r\n          );\r\n        })}\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default MovieGridView;\r\n","import { React } from \"react\";\r\nimport \"./styles.css\";\r\n\r\nconst MovieView = (props) => {\r\n  const { movieObj } = props.history.location.state;\r\n\r\n  const handleClick = async () => {\r\n    await fetch(`/trailer/${movieObj.id}`)\r\n      .then((response) => response.json())\r\n      .then(({ trailer }) => window.open(trailer));\r\n  };\r\n\r\n  return (\r\n    <>\r\n      <div className=\"movie-view-container\">\r\n        <div className=\"movie-view-content\">\r\n          <div className=\"movie-view-main\">\r\n            <div className=\"movie-view-poster-container\">\r\n              <img\r\n                className=\"movie-view-poster\"\r\n                src={movieObj.poster_path}\r\n                alt=\"\"\r\n              />\r\n              <div className=\"movie-view-trailer-label\" onClick={handleClick}>\r\n                View Trailer\r\n              </div>\r\n            </div>\r\n            <div className=\"movie-view-information\">\r\n              <div className=\"movie-view-title\">{movieObj.title}</div>\r\n              <div className=\"movie-view-info-header\">Score:</div>\r\n              <div className=\"movie-view-info-content\">\r\n                {movieObj.vote_average} / 10\r\n              </div>\r\n              <div className=\"movie-view-info-header\">Genres:</div>\r\n              <div>\r\n                {movieObj.genre_list.map((genre, index) => {\r\n                  return <div className=\"movie-view-info-content\">{genre}</div>;\r\n                })}\r\n              </div>\r\n              <div className=\"movie-view-info-header\">Summary:</div>\r\n              <div className=\"movie-view-info-content\">{movieObj.overview}</div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default MovieView;\r\n","import { CircularProgress } from \"@material-ui/core\";\r\nimport { React, useEffect, useState } from \"react\";\r\nimport { MovieGridView } from \"../../components/MoviesPage\";\r\nimport \"./styles.css\";\r\n\r\nconst Movies = (props) => {\r\n  const { filter } = props.history.location.state;\r\n  const [movieList, setMovieList] = useState([]);\r\n  const [loading, setLoading] = useState(true);\r\n\r\n  let type = filter;\r\n  if (type === \"comingsoon\") {\r\n    type = \"coming soon\";\r\n  } else if (type.includes(\"search/\")) {\r\n    type = `results for \"${type.substring(7)}\":`;\r\n  }\r\n\r\n  useEffect(async () => {\r\n    setLoading(true);\r\n    await fetch(`/${filter}`)\r\n      .then((response) => response.json())\r\n      .then(({ results }) => {\r\n        setMovieList(results);\r\n      });\r\n    let id = setTimeout(() => {\r\n      setLoading(false);\r\n    }, 1000);\r\n    return () => {\r\n      clearTimeout(id);\r\n    };\r\n  }, [filter]);\r\n\r\n  return (\r\n    <>\r\n      <div className=\"movie-page-container\">\r\n        {loading ? (\r\n          <CircularProgress class=\"progress-icon\" />\r\n        